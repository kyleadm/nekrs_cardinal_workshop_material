// Fluid Domain Boundary Conditions
// -----------------------------------------------------------

void scalarDirichletConditions(bcData * bc)
{
  // wall boundary condition (fluid-solid shared interface)
  if (bc->id == 1)
  {
    bc->s = bc->usrwrk[bc->idM + 1 * bc->fieldOffset]; // temperature sent from MOOSE
  }
}
